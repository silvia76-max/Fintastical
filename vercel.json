{
  "version": 2,
  "builds": [
    {
      "src": "frontend/dist/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "backend/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/stocks",
      "dest": "/backend/server.js"
    },
    {
      "src": "/.*",
      "dest": "/frontend/dist/index.html"
    }
  ]
}
