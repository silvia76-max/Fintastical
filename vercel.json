{
  "version": 2,
  "builds": [
    {
      "src": "backend/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/dist/**/*",
      "use": "@vercel/static"
    }
  ],
  "headers": [
    {
      "source": "/Fintastical/assets/(.*)\\.css",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/Fintastical/assets/(.*)\\.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/api/(.*)\\.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/api/stock/(.*)",
      "dest": "/backend/server.js"
    },
    {
      "src": "/api/db",
      "dest": "/backend/server.js"
    },
    {
      "src": "/Fintastical/(.*)",
      "dest": "/frontend/dist/index.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "/backend/server.js"
    }
  ]
}