{
  "version": 2,
  "headers": [
    {
      "source": "/Fintastical/assets/(.*)\\.css",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/Fintastical/assets/(.*)\\.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/api/(.*)\\.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/stock/(.*)",
      "destination": "/backend/server.js"
    },
    {
      "source": "/api/db",
      "destination": "/backend/server.js"
    },
    {
      "source": "/Fintastical/(.*)",
      "destination": "/frontend/dist/index.html"
    }
  ]
}
